<?xml version="1.0" encoding="utf-8"?>
<ripple
    xmlns:andorid="http://schemas.android.com/apk/res/android"
    andorid:color="@color/greenPress">

    <item andorid:id="@+id/background">
        <shape andorid:shape="rectangle">
            <solid andorid:color="@color/green"/>
